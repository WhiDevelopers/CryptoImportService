{
    "name": "cryptoimporter",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "dev": "tsc && nodemon ./bin/www",
        "build": "tsc -p .",
        "watch-ts": "tsc -w",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\""
    },
    "dependencies": {
        "cookie-parser": "^1.4.4",
        "core-util-is": "^1.0.2",
        "debug": "^2.6.9",
        "express": "^4.18.2",
        "http-errors": "^1.6.3",
        "jade": "^1.11.0",
        "mongodb": "^3.5.5",
        "morgan": "^1.9.1"
    },
    "devDependencies": {
        "@types/mongodb": "^3.5.3",
        "@types/express": "^4.17.3",
        "@types/node": "^13.9.3",
        "nodemon": "^2.0.2",
        "ts-node": "^8.8.1",
        "concurrently": "^3.5.1",
        "typescript": "^3.8.3"
    }
}
